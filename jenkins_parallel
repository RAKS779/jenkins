pipeline
{
    agent any

    stages
    {
          stage('Test1_outside') {
            steps
            { sh 'sleep 4'
            }
            }


       stage('parallel_test'){
             Parallel {
            stage('Test1') {
            steps {
                 sh 'sleep 2'
            }
            }
             stage('Test2') {
            steps {
                sh 'sleep 2'
            }
            }
             }
       }
        
        
        stage('Test2_oustside') {
            steps
            { sh 'sleep 4'
            }
            }
        
    }














    
}