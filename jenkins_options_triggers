pipeline
{
    agent any

options {
    ansiColor('xterm')
    buildDiscarder(logRotator(numToKeepStr: '5'))
    disableConcurrentBuilds(abortPrevious: true)
    retry(3)
    timeout(time: 1, unit: 'SECONDS')
    timestamps()
}



    stages
    {
        stage('options_trigger')
        {
            steps{
                echo " inside stages and steps"
            }
        }
    }
}