pipeline
{
    agent {
      label ' slave1'
    }


    stages{
         stage('Stage1')
         {
          sh 'sleep 4 '
            steps{
           echo " this is first stage"
         }
         }

         stage('Stage2')
         {
          sl 'sleep 7'
            steps{
           echo " this is second stage"
           
         }
         }
    }

}